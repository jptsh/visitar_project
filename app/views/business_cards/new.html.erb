


<div class="container">
  <div class="row">
    <div class="col-xs-12 col-sm-4 col-sm-offset-4">

      <div class="form-login">
        <h1>Log In</h1>
        <%= simple_form_for(@business_card) do |f| %>
          <%= f.input :firstname, id:"input1" %>
          <%= f.input :lastname %>
          <%= f.input :jobtitle %>
          <%= f.input :company %>
          <%= f.input :phone %>
          <%= f.input :email %>
          <%= f.input :website %>
          <%= f.submit %>
        <% end %>
      </div>
      <!-- Insert form here -->
    </div>
  </div>
</div>


